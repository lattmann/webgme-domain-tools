    "test_karma": "node ./node_modules/karma/bin/karma start karma.conf.js --single-run --browsers Firefox",
    "jsdoc": "node_modules/.bin/jsdoc src -r",
    "plugin": "node node_modules/webgme/bin/run_plugin.js -c config.json"
